{
  "name": "country-info-app",
  "version": "1.0.0",
  "scripts": {
    "install:all": "pnpm install",
    "start:backend": "pnpm --filter country-info-backend start:dev",
    "start:frontend": "pnpm --filter country-info-frontend dev",
    "start": "pnpm concurrently \"pnpm run start:backend\" \"pnpm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "pnpm": "^8.0.0"
  }
}
